{
    "name": "devolt/laravel-modular",
    "description": "",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/TheDevolt/laravel-restful",
    "support": {
        "issues": "https://github.com/TheDevolt/laravel-restful/issues",
        "source": "https://github.com/TheDevolt/laravel-restful"
    },
